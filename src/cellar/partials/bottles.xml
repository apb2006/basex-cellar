<div class="row-fluid" data-ng-init="setWindowTitle('Bottles')">
	<h2>Bottle pictures</h2>
	<ul class="media-list winelist">
		<li class="media thumbnail" data-ng-repeat="bottle in bottles">
			<a href="#/users/{{  user.id }}" class="pull-left">
				<img class="media-object img-circle" src="pics/user1.png" />
			</a>
			<div class="media-body">
				<h4 class="media-heading">{{ user.name }}</h4>
				<div>The id is: {{ user.id }}</div>
			</div>
		</li>
	</ul>
</div>